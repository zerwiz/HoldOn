{
  "name": "hold-on",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android --profile preview",
    "build:android:local": "eas build --platform android --profile preview --local",
    "build:android:production": "eas build --platform android --profile production",
    "build:android:production:aab": "eas build --platform android --profile production --type app-bundle",
    "build:android:native": "expo prebuild --platform android && cd android && ./gradlew assembleRelease",
    "build:ios": "eas build --platform ios"
  },
  "dependencies": {
    "expo": "~49.0.0",
    "expo-haptics": "~12.4.0",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "sharp": "^0.32.6"
  },
  "private": true
}
